{
  "name": "abexto/yii2-ydoctrine-base",
  "description": "yDoctrine Base Classes",
  "keywords": ["yii", "doctrine"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Andreas Prucha, Abexto - Helicon Software Development",
      "email": "prucha@helicon.co.at"
    }
  ],
  "repositories": [
    {
            "type": "vcs",
            "url":  "https://github.com/abexto/yii2-yepa-phpunit.git"
    }
  ],
  "require": {
    "doctrine/orm": "*",
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "abexto/yii2-yepa-phpunit": "*@dev"
  },
  "autoload": {
    "psr-4": {
